{
    "name": "material-confirm-dialog",
    "version": "0.1.0",
    "main": "dist/index.js",
    "module": "dist/index.js",
    "description": "React context wrapper for a Material-UI confirm dialog.",
    "private": false,
    "keywords": [
        "material",
        "confirm",
        "dialog"
    ],
    "devDependencies": {
        "@babel/cli": "^7.16.8",
        "@babel/core": "^7.16.7",
        "@babel/preset-react": "^7.16.7",
        "@babel/preset-typescript": "^7.18.6",
        "@types/react": "^18.0.28",
        "@types/react-dom": "^18.0.11",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-scripts": "5.0.1",
        "typescript": "^4.9.5"
    },
    "scripts": {
        "dev": "react-scripts start",
        "build": "babel src/lib/* --out-dir dist --copy-files --extensions \".tsx\",\".ts\" && tsc --declaration --emitDeclarationOnly --outDir dist --lib esnext,dom,dom.iterable --jsx react-jsx --moduleResolution node --isolatedModules --allowSyntheticDefaultImports src/lib/index.ts"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "dependencies": {
        "@emotion/react": "^11.10.6",
        "@emotion/styled": "^11.10.6",
        "@mui/material": "^5.11.9"
    }
}